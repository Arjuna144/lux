(;import lux)

## Signatures
(defsig #export (Dict d)
  (: (All [k v]
       (-> k (d k v) (Maybe v)))
     get)
  (: (All [k v]
       (-> k v (d k v) (d k v)))
     put)
  (: (All [k v]
       (-> k (d k v) (d k v)))
     remove))
