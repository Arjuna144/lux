(;alias-lux)

(def (filter p xs)
  (All [a] (-> (-> a Bool) (List a) (List a)))
  (case xs
    #;Nil
    (list)

    (#;Cons [x xs'])
    (if (p x)
      (list& x (filter p xs'))
      (filter p xs'))))

